{
  "name": "Yuji Extension",
  "short_name": "Yuji",
  "description": "Interact with yuji.app from any page",
  "version": "1.0.1",
  "version_name": "1.0.1 Beta",
  "minimum_chrome_version": "88",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "/pages/youtube/popup.html"
  },
  "icons": {
    "512": "/icon.png"
  },
  "offline_enabled": false,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://*.yuji.app/*"
  ],
  "optional_host_permissions": [
    "https://*.youtube.com/watch?.*",
    "https://*.youtube.com/shorts/.*",
    "https://*.discord.com/channels/.*"
  ],
  "key": ""
}